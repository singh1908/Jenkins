    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hii') {
            steps {
                echo 'Hii World'
            }
        }
        stage('Heyy') {
            steps {
                echo 'Heyy World'
            }
        }
    }
}
